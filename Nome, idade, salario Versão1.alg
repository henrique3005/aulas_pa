algoritmo "TrocarValores"
// Função :
// Autor :
// Data : 15/03/2022
// Seção de Declarações 
var
nome:caracter
idade:inteiro
salario,porcentagem,total:real
inicio
// Seção de Comandos 
escreval("digite seu nome: ",nome)
leia(nome)
escreval("digite sua idade: ")
leia(idade)
escreval("digite seu salario: ")
leia(salario)

fimalgoritmo
